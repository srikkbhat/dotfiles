$1 = strtod($2, NULL);
endsnippet

snippet strtomd "convert char array to many double"
double $1;
char * $2;
$1 = strtod($3, &$2);
$1 = strtod($2, NULL);
endsnippet

snippet strtoi "convert char array to int"
